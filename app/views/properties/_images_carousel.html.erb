    <button type="button" data-bs-target="#propertyImages" data-bs-slide-to="12" aria-label="Slide 13"></button>
    <button type="button" data-bs-target="#propertyImages" data-bs-slide-to="13" aria-label="Slide 14"></button>
    <button type="button" data-bs-target="#propertyImages" data-bs-slide-to="14" aria-label="Slide 15"></button>
    <button type="button" data-bs-target="#propertyImages" data-bs-slide-to="15" aria-label="Slide 16"></button>
    <button type="button" data-bs-target="#propertyImages" data-bs-slide-to="16" aria-label="Slide 17"></button>
  </div>
  <div class="carousel-inner rounded">
    <div class="carousel-item active">
      <%= image_tag "master2.jpeg", alt: "Master bedroom", class: "d-block w-100" %>
    </div>
    <div class="carousel-item">
      <%= image_tag "master1.jpeg", alt: "Master bedroom from door", class: "d-block w-100" %>
