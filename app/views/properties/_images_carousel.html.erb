    </div>
    <div class="carousel-item">
      <%= image_tag "kitchen-and-tv1.jpeg", alt: "Kitchen and tv", class: "d-block w-100" %>
    </div>
    <div class="carousel-item">
      <%= image_tag "living-room1.jpeg", alt: "Living room 1", class: "d-block w-100" %>
    </div>
    <div class="carousel-item">
      <%= image_tag "living-room2.jpeg", alt: "Living room 2", class: "d-block w-100" %>
    </div>
    <div class="carousel-item">
      <%= image_tag "living-room3.jpeg", alt: "Living room 3", class: "d-block w-100" %>
